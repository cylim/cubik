{
  "name": "@cubik/database",
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "db:gen": "prisma generate --schema ./prisma/schema.prisma",
    "db:old:gen": "prisma generate --schema ./prisma/old.prisma",
    "db:push": "dotenv -e ../../.env -- npx prisma db push --schema ./prisma/schema.prisma",
    "db:old:push": "dotenv -e ../../.env -- npx prisma db pull --schema ./prisma/old.prisma",
    "db:pull": "dotenv -e ../../.env -- npx prisma db pull --schema ./prisma/schema.prisma",
    "db:old:pull": "dotenv -e ../../.env -- npx prisma db pull --schema ./prisma/old.prisma",
    "dev": "dotenv -e ../../.env -- npx prisma studio --port 5556"
  },
  "dependencies": {
    "@cubik/config": "workspace:*",
    "@prisma/client": "^5.6.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "dotenv-cli": "latest",
    "next": "^14.0.4"
  },
  "devDependencies": {
    "prisma": "^5.6.0",
    "typescript": "^5.3.2"
  }
}
